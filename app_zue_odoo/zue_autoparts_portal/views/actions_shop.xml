<odoo>
    <data>
        <template id="zue_autoparts_portal.products" inherit_id="website_sale.products"
                  name="Zue Autopartes Portal: Products">
            <xpath expr="//div[@id='wrap']" position="before">
                <!-- https://harvesthq.github.io/chosen/ -->
                <script src="https://code.jquery.com/jquery-3.6.4.min.js"/>
                <link rel="stylesheet" href="zue_autoparts_portal/static/chosen_v1.8.7/chosen.min.css"/>
                <script src="zue_autoparts_portal/static/chosen_v1.8.7/chosen.jquery.min.js"/>
                <style>
                    .modal-open #wrapwrap {
                    overflow: auto;
                    }
                </style>
                <script>
                    $(document).ready(function() {
                    $('.js-example-basic-multiple').chosen({
                    max_selected_options: 1,
                    no_results_text: "No se encontraron resultados!",
                    placeholder_text_multiple: "...",
                    width: "80%"
                    });
                    $('.js-example-basic-multiple').on('change', function () {
                    $(this).closest('form').submit();
                    });
                    });
                </script>
                <script>
                    document.addEventListener('DOMContentLoaded', function() {
                    document.body.classList.remove('modal-open');
                    const searchInputs = document.querySelectorAll('.zue-search');

                    searchInputs.forEach(input => {
                    input.addEventListener('input', function() {
                    const searchTerm = input.value.toLowerCase();
                    const filterId = input.getAttribute('data-filter-id');

                    const filterList = document.querySelector(`ul[data-filter-list="${filterId}"]`);
                    const items = filterList.getElementsByTagName('li');

                    for (let i = 0; i &lt; items.length; i++) {
                    const item = items[i];
                    const searchTermData = item.getAttribute('data-search-term');

                    if (searchTermData.includes(searchTerm)) {
                    item.style.display = '';
                    } else {
                    item.style.display = 'none';
                    }
                    }
                    });
                    });
                    });
                </script>
            </xpath>
        </template>

        <template id="zue_autoparts_portal.products_top_filters_zue"
                  name="Zue Autopartes Portal - Top Filtros">
            <div class="zue-top-filters mb-3">
                <form class="js_attributes" method="get"
