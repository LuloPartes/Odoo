                            <t t-set="title" t-value="'Tipo de vehículo'"/>
                            <t t-set="obj" t-value="obj_vehicle_type"/>
                            <t t-set="name" t-value="'vehicletype'"/>
                            <t t-set="active_search" t-value="True"/>
                            <t t-set="is_collapsible" t-value="True"/>
                            <t t-set="show_label" t-value="'field_name'"/>
                        </t>
                        <t t-call="zue_autoparts_portal.filter_autoparts_fleet_combo"
                           t-if="obj_stock_vehicle_type and obj_filter_parameterization.z_stock_vehicle_type">
                            <t t-set="selected" t-value="selected_stock_vehicletype"/>
                            <t t-set="title" t-value="'Tipo de vehículo'"/>
                            <t t-set="obj" t-value="obj_stock_vehicle_type"/>
                            <t t-set="name" t-value="'stock_vehicletype'"/>
                            <t t-set="active_search" t-value="True"/>
                            <t t-set="is_collapsible" t-value="True"/>
                            <t t-set="show_label" t-value="'field_display_name'"/>
