                                    <div t-if="obj_attributes_fleet_transmision" class="flex-fill p-2 bd-highlight"
                                         style="width: 50%;">
                                        <t t-call="zue_autoparts_portal.filter_autoparts_fleet">
                                            <t t-set="selected" t-value="selected_fleet_transmision"/>
                                            <t t-set="title" t-value="'Transmisión'"/>
                                            <t t-set="obj" t-value="obj_attributes_fleet_transmision"/>
                                            <t t-set="name" t-value="'fleet_transmision'"/>
                                            <t t-set="active_search" t-value="False"/>
                                            <t t-set="is_collapsible" t-value="False"/>
                                            <t t-set="show_label" t-value="'field_display_name_replace'"/>
                                        </t>
                                    </div>
                                </div>
                                <div class="d-flex flex-row bd-highlight mb-3">
                                    <div t-if="obj_attributes_fleet_traccion" class="flex-fill p-2 bd-highlight"
                                         style="width: 50%;">
                                        <t t-call="zue_autoparts_portal.filter_autoparts_fleet">
                                            <t t-set="selected" t-value="selected_fleet_traccion"/>
                                            <t t-set="title" t-value="'Tracción'"/>
                                            <t t-set="obj" t-value="obj_attributes_fleet_traccion"/>
                                            <t t-set="name" t-value="'fleet_traccion'"/>
                                            <t t-set="active_search" t-value="False"/>
                                            <t t-set="is_collapsible" t-value="False"/>
                                            <t t-set="show_label" t-value="'field_display_name_replace'"/>
                                        </t>
                                    </div>
                                    <div t-if="obj_attributes_fleet_tipofreno" class="flex-fill p-2 bd-highlight"
                                         style="width: 50%;">
                                        <t t-call="zue_autoparts_portal.filter_autoparts_fleet">
                                            <t t-set="selected" t-value="selected_fleet_tipofreno"/>
                                            <t t-set="title" t-value="'Tipo Freno'"/>
                                            <t t-set="obj" t-value="obj_attributes_fleet_tipofreno"/>
                                            <t t-set="name" t-value="'fleet_tipofreno'"/>
                                            <t t-set="active_search" t-value="False"/>
                                            <t t-set="is_collapsible" t-value="False"/>
                                            <t t-set="show_label" t-value="'field_display_name_replace'"/>
                                        </t>
                                    </div>
                                </div>
                                <div class="d-flex flex-row bd-highlight mb-3">
                                    <div t-if="obj_attributes_fleet_tipovehiculo" class="flex-fill p-2 bd-highlight"
                                         style="width: 50%;">
                                        <t t-call="zue_autoparts_portal.filter_autoparts_fleet">
                                            <t t-set="selected" t-value="selected_fleet_tipovehiculo"/>
                                            <t t-set="title" t-value="'Tipo de Vehículo'"/>
                                            <t t-set="obj" t-value="obj_attributes_fleet_tipovehiculo"/>
                                            <t t-set="name" t-value="'fleet_tipovehiculo'"/>
                                            <t t-set="active_search" t-value="False"/>
                                            <t t-set="is_collapsible" t-value="False"/>
                                            <t t-set="show_label" t-value="'field_display_name_replace'"/>
                                        </t>
                                    </div>
                                </div>
                                <div t-if="obj_attributes_fleet_otros" class="d-flex flex-row bd-highlight mb-3">
                                    <div class="flex-fill p-2 bd-highlight" style="min-height: 200px;">
                                        <h6 class="mb-0">
                                            Otros
                                        </h6>
                                        <div t-attf-class="pt-3">
                                            <ul class="nav nav-pills flex-column ml-0 flex-nowrap">
                                                <t t-foreach="obj_attributes_fleet_otros" t-as="obj_afo">
                                                    <li class="nav-item"
                                                        t-att-data-search-term="obj_afo.display_name.lower()">
                                                        <div>
                                                            <span t-esc="obj_afo.display_name"/>
                                                            <br/>
                                                            <select class="js-example-basic-multiple"
                                                                    data-placeholder="..."
                                                                    t-att-id="'fleet_otros|'+str(obj_afo.id)"
                                                                    multiple="multiple"
                                                                    name="fleet_otros">
                                                                <t t-set="unique_options_fleet_otros" t-value="set()"/>
                                                                <t t-foreach="obj_attributes_fleet_otros_values.filtered(lambda x: x.z_attribute_setting_id.id == obj_afo.id)"
                                                                   t-as="obj_afov">
                                                                    <t t-set="option_value"
                                                                       t-value="str(obj_afo.id)+'|'+str(obj_afov.z_value)"/>
                                                                    <t t-if="option_value not in unique_options_fleet_otros">
                                                                        <option t-att-value="str(obj_afo.id)+'|'+str(obj_afov.z_value)"
                                                                                t-att-selected="str(obj_afo.id)+'|'+str(obj_afov.z_value) in selected_fleet_otros">
                                                                            <span t-esc="obj_afov.z_value"/>
                                                                        </option>
                                                                        <t t-set="unique_options_fleet_otros"
                                                                           t-value="unique_options_fleet_otros.union({option_value})"/>
                                                                    </t>
                                                                </t>
                                                            </select>
                                                        </div>
                                                    </li>
                                                </t>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </li>
                    </t>
                </ul>
            </form>
        </template>

        <!-- FILTROS ZUE -->
        <template id="zue_autoparts_portal.products_top_filters" inherit_id="website_sale.products"
                  name="Zue Autopartes Portal - Top Filtros Inherit">
            <xpath expr="//div[hasclass('products_header')]" position="before">
                <div class="d-none d-lg-block position-sticky clearfix">
                    <t t-call="zue_autoparts_portal.products_top_filters_zue"/>
                </div>
            </xpath>
        </template>

        <template id="zue_autoparts_portal.products_filters" inherit_id="website_sale.products_attributes"
                  name="Zue Autopartes Portal - Aside Filtros Inherit">
            <xpath expr="//div[hasclass('o_wsale_products_grid_before_rail')]" position="inside">
                <t t-call="zue_autoparts_portal.products_filters_zue"/>
            </xpath>
        </template>

        <template id="zue_autoparts_portal.o_wsale_offcanvas_zue" inherit_id="website_sale.o_wsale_offcanvas"
                  name="OffcanvasZue">
            <xpath expr="//div[@id='o_wsale_offcanvas_content']" position="inside">
                <div style="overflow-y: scroll;" id="o_wsale_offcanvas_content_zue">
                    <t t-call="zue_autoparts_portal.products_top_filters_zue"/>
                    <t t-call="zue_autoparts_portal.products_filters_zue"/>
                </div>
            </xpath>
        </template>

        <template id="zue_autoparts_portal.filter_autoparts_fleet" name="Filtros autopartes y vehiculos: Ecommerce">
            <div class="d-flex justify-content-between align-items-center">
                <h6 class="mb-0">
                    <span t-esc="title"/>
                </h6>
            </div>
            <div class="pt-3">
                <div class="input-group input-group-sm mb-3">
                    <input type="text" class="form-control zue-search" placeholder="Search..."
                           t-att-data-filter-id="name"/>
                    <div class="input-group-append">
                        <span class="input-group-text bg-white border-0">
                            <i class="dri dri-search-l text-body"/>
                        </span>
                    </div>
                </div>
                <ul class="nav nav-pills flex-column ml-0 flex-nowrap" t-att-data-filter-list="name">
                    <t t-foreach="obj.sorted(key=lambda i: (i.id not in selected, i.display_name), reverse=False)"
                       t-as="o">
                        <li class="nav-item" t-att-data-search-term="o.display_name.lower()">
                            <div class="form-check mb-2 ml-1">
                                <input type="checkbox" t-att-id="name+'|'+str(o.id)" t-att-name="name"
                                       class="form-check-input" t-att-value="o.id"
                                       t-att-checked="'checked' if o.id in selected else None"/>
                                <label class="form-check-label w-100" t-attf-for="#{o.display_name}-#{o.id}">
                                    <div class="d-flex align-items-center justify-content-between">
                                        <div class="d-flex align-items-center">
                                            <t t-if="show_label == 'field_name'">
                                                <span t-esc="o.name"/>
                                            </t>
                                            <t t-elif="show_label == 'field_z_name'">
                                                <span t-esc="o.z_name"/>
                                            </t>
                                            <t t-elif="show_label == 'field_display_name_replace'">
                                                <span t-esc="o.display_name.replace(title+' | ','')"/>
                                            </t>
                                            <t t-else="">
                                                <span t-esc="o.display_name"/>
                                            </t>
                                        </div>
                                    </div>
                                </label>
                            </div>
                        </li>
                    </t>
                </ul>
            </div>
        </template>

        <template id="zue_autoparts_portal.filter_autoparts_fleet_combo"
                  name="Filtros autopartes y vehiculos: Ecommerce Combo">
            <div class="card rounded-0">
                <div class="card-header p-1 rounded-0 bg-primary text-light">
                    <h5 class="card-title text-center m-0">
                        <span t-esc="title"/>
                    </h5>
                </div>
                <div class="card-body p-0 rounded-0">
                    <select class="form-select rounded-0" t-att-id="name" t-att-name="name"
                            t-att-data-filter-list="name">
                        <option value="0">--Seleccione--</option>
                        <t t-foreach="obj.sorted(key=lambda i: (i.id not in selected, i.display_name), reverse=False)"
                           t-as="o">
                            <option t-att-id="o.id" t-att-value="o.id" t-att-data-search-term="o.display_name.lower()"
                                    t-att-selected="'selected' if o.id in selected else None">
                                <t t-if="show_label == 'field_name'">
                                    <span t-esc="o.name"/>
                                </t>
                                <t t-elif="show_label == 'field_z_name'">
                                    <span t-esc="o.z_name"/>
                                </t>
                                <t t-elif="show_label == 'field_display_name_replace'">
                                    <span t-esc="o.display_name.replace(title+' | ','')"/>
                                </t>
                                <t t-else="">
                                    <span t-esc="o.display_name"/>
                                </t>
                            </option>
                        </t>
                    </select>
                </div>
            </div>
        </template>

    </data>
</odoo>